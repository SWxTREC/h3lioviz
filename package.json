{
  "name": "h3lioviz",
  "version": "2.3.2",
  "scripts": {
    "changelog": "auto-changelog",
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "start:dev": "ng serve --configuration production,dev",
    "build": "ng build",
    "build:pages": "ng build --prod --output-path docs --base-href=/h3lioviz/",
    "build:prod": "ng build --configuration production --base-href=/h3lioviz/",
    "build:dev": "ng build --configuration production,dev --base-href=/h3lioviz/",
    "build:swpc": "ng build --configuration production,swpc --base-href=/h3lioviz/",
    "deploy": "npm run build:pages && cp docs/index.html docs/404.html && gh-pages -d docs",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:watch": "nodemon --exec \"ng lint\" --ext ts --ignore node_modules/",
    "e2e": "ng e2e",
    "preversion": "ng lint && ng test --no-watch --no-progress",
    "postversion": "git push origin && git push --tags origin && git push external main && git push --tags external main && git checkout dev && git merge origin/main && git push",
    "version": "auto-changelog -p && git add src/assets/markdown/CHANGELOG.md"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/builder": "^18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular-slider/ngx-slider": "^17.0.2",
    "@angular/animations": "^18.2.14",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.14",
    "@angular/compiler": "^18.2.14",
    "@angular/core": "^18.2.14",
    "@angular/forms": "^18.2.14",
    "@angular/localize": "^18.2.14",
    "@angular/material": "^18.2.14",
    "@angular/material-moment-adapter": "^18.2.14",
    "@angular/platform-browser": "^18.2.14",
    "@angular/platform-browser-dynamic": "^18.2.14",
    "@angular/router": "^18.2.14",
    "@ngx-pwa/local-storage": "^17.0.0",
    "angular-split": "^17.2.0",
    "auto-changelog": "^2.5.0",
    "babel-loader": "^9.1.3",
    "core-js": "^3.25.3",
    "dom-to-svg": "^0.12.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^46.10.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^42.0.0",
    "fast-json-stable-stringify": "^2.1.0",
    "file-saver": "^2.0.5",
    "highcharts": "~10.3.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "mediabunny": "^1.25.7",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "ngx-markdown": "^17.2.1",
    "ngx-moment": "^6.0.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.3.0",
    "rxjs": "^7.8.1",
    "sanitize-filename": "^1.6.3",
    "shader-loader": "^1.3.1",
    "streamsaver": "^2.0.6",
    "svg2pdf.js": "^2.5.0",
    "text-encoding": "^0.7.0",
    "tslib": "^2.0.0",
    "vtk.js": "^25.6.0",
    "web-streams-polyfill": "^3.3.3",
    "worker-loader": "^3.0.8",
    "wslink": "^1.6.6",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.2",
    "@angular-devkit/build-angular": "^18.2.21",
    "@angular/cli": "^18.2.21",
    "@angular/compiler-cli": "^18.2.14",
    "@angular/language-service": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.170",
    "@types/node": "^18.19.8",
    "ajv": "^6.12.6",
    "autoprefixer": "^10.4.17",
    "gh-pages": "^2.2.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "typescript": "~5.4.5"
  },
  "auto-changelog": {
    "commitLimit": false,
    "handlebarsSetup": "./changelog-helpers.js",
    "includeBranch": [
      "main"
    ],
    "output": "src/assets/markdown/CHANGELOG.md",
    "startingVersion": "2.3.2",
    "template": "changelog-template.hbs",
    "unreleased": false
  }
